    {
      "id": 6,
      "type": "timeseries",
      "title": "Cluster Memory Usage",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(node_memory_MemTotal_bytes - node_memory_MemAvailable_bytes)",
          "legendFormat": "Used"
        },
        {
          "refId": "B",
          "expr": "sum(node_memory_MemTotal_bytes)",
          "legendFormat": "Total"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
